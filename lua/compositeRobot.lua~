function DisabledInit()
    if AutonomousDisabledInit then
        AutonomousDisabledInit()
    end
    if TeleopDisabledInit then
        TeleopDisabledInit()
    end
end

function DisabledPeriodic()
    if AutonomousDisabledPeriodic then
        AutonomousDisabledPeriodic()
    end
    if TeleopDisabledPeriodic then
        TeleopDisabledPeriodic()
    end
end