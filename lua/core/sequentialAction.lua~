local function seqActInit(self)
    self.actNum = 1
    self.actions[1].Initialize()
end

local function seqActExec(self)
    if self.actNum == 0 then return end
    self.actions[self.actNum].Execute()
    
end

local function createSeqAct(...)
    local seqAct = {}
    seqAct.actions = {...}
    seqAct.actNum = 0